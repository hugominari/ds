{
    "name": "appstract/laravel-blade-directives",
    "description": "Handy Blade directives",
    "keywords": [
        "appstract",
        "laravel-blade-directives"
    ],
    "homepage": "https://github.com/appstract/laravel-blade-directives",
    "license": "MIT",
    "authors": [
        {
            "name": "Gijs Jorissen",
            "email": "hello@appstract.team",
            "homepage": "https://appstract.team",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "Appstract\\BladeDirectives\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Appstract\\BladeDirectives\\Test\\": "tests",
            "Appstract\\BladeDirectives\\Test\\App\\": "tests/laravel/app"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appstract\\BladeDirectives\\BladeDirectivesServiceProvider"
            ]
        }
    }
}
